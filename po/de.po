#: ../virt-convert:54
msgid "Output format, e.g. 'virt-image'"
msgstr "Ausgabeformat, z.B. 'virt-image'"

#: ../virt-convert:56
msgid "Output disk format"
msgstr "Ausgabe Datenträgerformat"

#: ../virt-convert:69
